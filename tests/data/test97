#
# Client-side
<client>
<server>
http
</server>
 <name>
HTTP POST with custom content-type
 </name>
 <command>
 -d "hejsanallabarn" -H "Content-Type: silly/type" http://%HOSTIP:%HOSTPORT/97
</command>
</test>

#
# Verify data after the test has been "shot"
<verify>
<strip>
^User-Agent:.*
</strip>
<protocol nonewline=yes>
POST /97 HTTP/1.1
Host: 127.0.0.1:8999
Pragma: no-cache
Accept: */*
Content-Type: silly/type
Content-Length: 14

hejsanallabarn
</protocol>
</verify>
