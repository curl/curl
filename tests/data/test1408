User-Agent: curl/%VERSION
Accept: */*

GET /path/%TESTNUMBER0002 HTTP/1.1
Host: %HOST6IP:%HTTP6PORT
User-Agent: curl/%VERSION
Accept: */*
Cookie: time=1

</protocol>
</verify>
